<?xml version="1.0" encoding="UTF-8"?>
<!-- Value between [] brackets, for example [#FFFFFF] shows default value which is used if this parameter is not set -->
<!-- This means, that if you are happy with this value, you can delete this line at all and reduce file size -->
<!-- value or explanation between () brackets shows the range or type of values you should use for this parameter -->
<!-- the top left corner has coordinates x = 0, y = 0                                                                -->
<!-- "!" before x or y position (for example: <x>!20</x>) means that the coordinate will be calculated from the right side or the bottom -->

<settings>
	
	<font>Tahoma</font>
	<text_size>14</text_size>
	<redraw>true</redraw>
	<thousands_separator>,</thousands_separator>
	<decimals_separator>.</decimals_separator>
	
	<background>
		<alpha>100</alpha>
		<color>ffffff</color>
	</background>

	<plot_area>
		<margins>
			<top>40</top>
			<left>80</left>
			<right>80</right>
			<bottom>160</bottom>
		</margins>
	</plot_area>
	
	<grid>
		<x>
			<alpha>10</alpha>
			<approx_count>7</approx_count>
		</x>
		<y_left>
			<alpha>10</alpha>
		</y_left>
	</grid>
	
	<axes>
		<x>
			<width>1</width>
			<color>999999</color>
		</x>
		<y_left>
			<width>1</width>
			<color>999999</color>
			<type>stacked</type>
		</y_left>
	</axes>

	<labels>
		<label>
			<x>10</x>
			<y>50%</y>
			<text>Number of Stories</text>
			<rotate>true</rotate>
			<text_color>999999</text_color>
		</label>
	</labels>
	
	<indicator>
		<color>000000</color>
		<x_balloon_text_color>FFFFFF</x_balloon_text_color>
		<line_alpha>50</line_alpha>
		<selection_color>0D8ECF</selection_color>
		<selection_alpha>20</selection_alpha>
	</indicator>
	
	<zoom_out_button>
		<text_color_hover>FF0F00</text_color_hover>
	</zoom_out_button>

	<error_messages>
		<color>AAAAAA</color>
		<text_color>222222</text_color>
	</error_messages>

	<strings>
		<no_data>No data has been collected for the selected period</no_data>
		<error_in_data_file>No data has been collected for the selected period</error_in_data_file>
	</strings>

</settings>